{
	"compilerOptions": {
	  "lib": ["es2018"],
	  "module": "commonjs",
	  "target": "es2018",
	  "baseUrl": ".",
	  "strict": true,
	  "esModuleInterop": true,
	  "skipLibCheck": true,
	  "forceConsistentCasingInFileNames": true,
	  "sourceMap": true,
	  "outDir": "dist"
	},
	"include": ["src/**/*"],
	"exclude": ["node_modules", "**/*.spec.ts"]
  }